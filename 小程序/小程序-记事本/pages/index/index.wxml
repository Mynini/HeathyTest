<view class="page">
   <view class="list" wx:for="{{lists}}"  bindtap="edit"  data-id="{{item.id}}">
     <view class='content'>{{item.content}}</view>
     <view class="time">{{item.time}}</view>
   </view> 

   <view class="edit" bindtap="add">添加</view>
   <view class='clear' bindtap="clear">清除缓存</view>

   
</view>